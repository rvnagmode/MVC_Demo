<h2>Product List</h2>
<style>
    .card{
        border:double;
    }
    
    .table
    {
        border:solid;
        margin-top:15px;

    }
    </style>
<div class="card">
  <div class="card-body">
<table class="table table-bordered" >
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
       
      
 @foreach(MVC_Demoo.Models.Product item in ViewBag.Productlist)
    {
     
     <tr>
     
      <td>@item.Id</td>
      <td>@item.Name</td>
      <td>@item.Price</td>
    </tr>
   
    }
    
  </tbody>
</table>
  </div>
  </div>
    